{
  "name": "brows",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.example.my-electron-app",
    "productName": "My Electron App",
    "directories": {
      "output": "dist"
    },
    "files": [
      "index.js"
    ],
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "extraResources": [
      "node_modules/electron-reload/**",
      "node_modules/chokidar/**",
      "node_modules/readdirp/**"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^3.5.3",
    "electron": "^23.2.1",
    "electron-builder": "^23.6.0",
    "electron-reload": "^2.0.0-alpha.1",
    "picomatch": "^2.3.1",
    "readdirp": "^3.6.0"
  }
}
